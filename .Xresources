URxvt.background: #232323
URxvt.foreground: #A0A0A0

!BLK
URxvt.color0:     #303030
URxvt.color8:     #686868

!RED
URxvt.color1:     #BF3F34
URxvt.color9:     #FF6C5F

!GRN
URxvt.color2:     #6D9E3F
URxvt.color10:    #A3D572

!YEL
URxvt.color3:     #A88E42
URxvt.color11:    #E0CC8F

!BLU
URxvt.color4:     #4863B5
URxvt.color12:    #98ADEB

!MAG
URxvt.color5:     #75507B
URxvt.color13:     #AD7FA8

!CYN
URxvt.color6:     #3BA275
URxvt.color14:    #75DAA9

!WHT
URxvt.color7:     #CFCFCF
URxvt.color15:    #FFFFFF

URxvt.iconFile: /usr/local/share/icons/Faenza/apps/scalable/xterm.svg
URxvt.font: xft:Source Code Pro:size=14:medium:antialias=true:hinting=true
URxvt.scrollBar: false
URxvt.perl-ext-common: default,font-size,clipboard,url-select
URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.clipboard.autocopy: true

URxvt.url-select.launcher: /usr/bin/xdg-open
URxvt.url-select.underline: true

URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal

URxvt.keysym.C-C:   perl:clipboard:copy
URxvt.keysym.C-V:   perl:clipboard:paste

URxvt.keysym.M-u: perl:url-select:select_next


